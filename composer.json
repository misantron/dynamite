{
    "name": "misantron/dynamite",
    "description": "AWS DynamoDB data fixtures loader",
    "keywords": [
        "php",
        "dynamodb",
        "async-aws",
        "aws-sdk-php",
        "data-fixtures"
    ],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "psr/log": "^3.0",
        "symfony/console": "^6.4 || ^7.0",
        "symfony/property-access": "^6.4 || ^7.0",
        "symfony/serializer": "^6.4 || ^7.0",
        "symfony/validator": "^6.4 || ^7.0",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "async-aws/dynamo-db": "^3.6",
        "aws/aws-sdk-php": "3.351.2",
        "fakerphp/faker": "^1.24",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpstan/phpstan-strict-rules": "^2.0",
        "phpunit/phpunit": "^11.5 || ^12.0",
        "rector/rector": "^2.0",
        "symfony/error-handler": "^6.4 || ^7.0",
        "symplify/easy-coding-standard": "^12.5",
        "vimeo/psalm": "^6.12"
    },
    "suggest": {
        "async-aws/dynamo-db": "Async first DynamoDb client from Symfony Core developers",
        "aws/aws-sdk-php": "Official PHP AWS SDK library"
    },
    "autoload": {
        "psr-4": {
            "Dynamite\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dynamite\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.2.99"
        },
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
