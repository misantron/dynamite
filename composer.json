{
    "name": "misantron/dynamite",
    "description": "AWS DynamoDB data fixtures loader",
    "keywords": [
        "php",
        "async-aws",
        "dynamodb",
        "data-fixtures"
    ],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "async-aws/dynamo-db": "^1.3",
        "psr/log": "^1.0 || ^2.0 || ^3.0",
        "symfony/property-access": "^5.4 || ^6.0",
        "symfony/serializer": "^5.4 || ^6.0",
        "symfony/validator": "^5.4 || ^6.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.19",
        "phpstan/phpstan": "^1.8",
        "phpstan/phpstan-phpunit": "^1.1",
        "phpstan/phpstan-strict-rules": "^1.3",
        "symfony/phpunit-bridge": "^5.4 || ^6.0",
        "symplify/easy-coding-standard": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Dynamite\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dynamite\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    }
}
