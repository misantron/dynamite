{
    "name": "misantron/dynamite",
    "description": "AWS DynamoDB data fixtures loader",
    "keywords": [
        "php",
        "dynamodb",
        "async-aws",
        "aws-sdk-php",
        "data-fixtures"
    ],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "psr/log": "^2.0 || ^3.0",
        "symfony/console": "^5.4 || ^6.4 || ^7.0",
        "symfony/property-access": "^5.4 || ^6.4 || ^7.0",
        "symfony/serializer": "^5.4 || ^6.4 || ^7.0",
        "symfony/validator": "^5.4 || ^6.4 || ^7.0",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "async-aws/dynamo-db": "^3.1",
        "aws/aws-sdk-php": "3.298.2",
        "fakerphp/faker": "^1.23",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpstan/phpstan-strict-rules": "^1.5",
        "phpunit/phpunit": "^10.5",
        "symfony/error-handler": "^5.4 || ^6.4 || ^7.0",
        "symplify/easy-coding-standard": "^12.1",
        "vimeo/psalm": "^5.18"
    },
    "suggest": {
        "async-aws/dynamo-db": "Async first DynamoDb client from Symfony Core developers",
        "aws/aws-sdk-php": "Official PHP AWS SDK library"
    },
    "autoload": {
        "psr-4": {
            "Dynamite\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dynamite\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.1.99"
        },
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
