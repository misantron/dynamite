{
    "name": "misantron/dynamite",
    "description": "AWS DynamoDB data fixtures loader",
    "keywords": [
        "php",
        "dynamodb",
        "async-aws",
        "aws-sdk-php",
        "data-fixtures"
    ],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "psr/log": "^2.0 || ^3.0",
        "symfony/property-access": "^5.4 || ^6.0",
        "symfony/serializer": "^5.4 || ^6.0",
        "symfony/validator": "^5.4 || ^6.0"
    },
    "require-dev": {
        "async-aws/dynamo-db": "^1.3",
        "aws/aws-sdk-php": "^3",
        "fakerphp/faker": "^1.21",
        "phpstan/phpstan": "^1.9",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpstan/phpstan-strict-rules": "^1.4",
        "phpunit/phpunit": "^10.0",
        "symfony/error-handler": "^5.4 || ^6.0",
        "symplify/easy-coding-standard": "^11.2",
        "vimeo/psalm": "^5.6"
    },
    "suggest": {
        "async-aws/dynamo-db": "Async first DynamoDb client from Symfony Core developers",
        "aws/aws-sdk-php": "Official PHP AWS SDK library"
    },
    "autoload": {
        "psr-4": {
            "Dynamite\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dynamite\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    }
}
